{
  "name": "@userdashboard/stripe-connect",
  "version": "1.0.18",
  "scripts": {
    "test": "mocha src --recursive --no-colors --retries 3 --timeout 120000 --slow 120000",
    "test2": "mocha src --recursive --no-colors --full-trace --retries 3 --timeout 120000 --slow 120000"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/userdashboard/stripe-connect.git"
  },
  "license": "SPDX(MIT OR Unlicense)",
  "main": "index.js",
  "dependencies": {
    "@userdashboard/maxmind-geoip": "^1.0.29",
    "stripe": "^7.8.0"
  },
  "devDependencies": {
    "@userdashboard/dashboard": "^1.0.37",
    "puppeteer": "^1.19.0"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "dashboard": {
    "title": "Dashboard",
    "modules": [
      "@userdashboard/stripe-connect",
      "@userdashboard/maxmind-geoip"
    ],
    "server": [
      "/src/server/bind-ip.js",
      "/src/server/bind-stripekey.js"
    ],
    "menus": {
      "administrator": [
        {
          "href": "/administrator/connect",
          "text": "Stripe Connect module",
          "object": "link"
        }
      ],
      "account": [
        {
          "href": "/account/connect",
          "text": "Stripe Connect accounts",
          "object": "link"
        }
      ]
    }
  }
}
